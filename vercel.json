    {
      "version": 2,
      "functions": {
        "api/index.php": {
          "runtime": "vercel-php@0.7.1" // Adjust runtime version based on your PHP version
        }
      },
      "routes": [
        {
          "src": "/build/(.*)",
          "dest": "/build/$1"
        },
        {
          "src": "/(.*)",
          "dest": "/api/index.php"
        }
      ],
      "outputDirectory": "public"
    }
